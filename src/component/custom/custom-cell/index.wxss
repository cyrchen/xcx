
.top{
  border-top: 1rpx solid #eeeeee;
}
.bottom{
  border-bottom: 1rpx solid #eeeeee;
}
.custom_cell{
  display: flex;
  flex-direction: row;
  align-items: stretch;
  margin-left: 40rpx;
  height: 100rpx;
}
.cell_icon{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-right: 24rpx;
  background: #fff;
}
.cell_icon image{
  width: 46rpx;
  height: 46rpx;
}
.cell_content{
  flex: 1;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  padding-right: 40rpx;
}

.title{
   
}
.title text{
  font-size: 30rpx;
  color: #333333;
}

.cell-right{
  display: flex;
  flex-direction: row;
  align-items: center;
  color: #B3B3B3;
}


.textLink_right_icon{
  margin-left: 20rpx;
  line-height:0;
  color: #B3B3B3;
  font-weight: 400;
}
.textLink_right_text{
  font-size: 28rpx;
  line-height:0rpx;

}



